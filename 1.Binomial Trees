#Binomial Option Pricing Model
import numpy as np 
#in this tutorial, we will be pricing a european call option

from functools import wraps
from time import time 

def timing(f):
   @wraps(f)
   def wrap(*args, **kw):
      ts= time()
      result = f(*args, **kw)                   #Lets make a generic timing wrapper function to benchmark our to binomial models
      te = time()
      print('func:%r args:[%r, %r] took: %2.4f sec' % \
            (f.__name__, args, kw, te-ts))
      return result
   return wrap
